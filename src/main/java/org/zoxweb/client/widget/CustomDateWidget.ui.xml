<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style type="org.zoxweb.client.widget.CustomDateWidget.Style">
		.error {
			border: 1px solid red;	
		}
		.valid {
	
		}
	</ui:style>
	<g:HTMLPanel width="100%" height="100%">
		<g:HorizontalPanel ui:field="hp" width="15EM" height="2EM" horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_MIDDLE">
			<g:ListBox visibleItemCount="1" ui:field="lbYear" width="100%" height="100%"/>
			<g:ListBox visibleItemCount="1" ui:field="lbMonth" width="100%" height="100%"/>
			<g:ListBox visibleItemCount="1" ui:field="lbDay" width="100%" height="100%"/>
		</g:HorizontalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 
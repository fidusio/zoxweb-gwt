<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<g:HorizontalPanel>
	<g:VerticalPanel>
		<g:HTML wordWrap="true"><b>Network Settings</b></g:HTML>
		<g:Grid ui:field="gData">
			<g:row>
				<g:customCell>
				 <g:HTML>Select Network</g:HTML>	
				</g:customCell>
				<g:customCell>
				<g:ListBox visibleItemCount="1" ui:field="lbNetwokInterface" width="11EM" height=""/>
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
				 <g:HTML>Select Type</g:HTML>	
				</g:customCell>
				<g:customCell>
				<g:ListBox visibleItemCount="1" ui:field="lbInetProto" width="11EM" height=""/>
				</g:customCell>
			</g:row>
			
		</g:Grid>
		<g:HorizontalPanel>
			<g:Button ui:field="bSave">Save</g:Button>
			<g:Cell horizontalAlignment="ALIGN_RIGHT">
				<g:Button ui:field="bCancel">Cancel</g:Button>
			</g:Cell>
		 </g:HorizontalPanel>
	</g:VerticalPanel>
	<g:VerticalPanel>
		<g:HTML wordWrap="true"><b>Current Configuration Info</b></g:HTML>
		<g:Grid ui:field="gInfo"/>
	</g:VerticalPanel>
	</g:HorizontalPanel>
</ui:UiBinder> 
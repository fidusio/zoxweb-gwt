<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style type="org.zoxweb.client.widget.NVEntityContainerDAOWidget.Style">
		.background {
			background-color: #FFFFFF;
		}
		.flexTable_RowDivider {
			border-bottom: 1px solid #BDBDBD;
		}
		.scrollPanel {
			overflow-x: hidden !important;
			overflow-y: scroll;
		}
		.header {
			font-weight: bold;
			font-size: 1.1EM;
			color: #2E2E2E;
		}
	</ui:style>
	<g:HTMLPanel width="100%" height="100%">
		<g:FlexTable width="40EM" height="100%" ui:field="flexTable" styleName="{style.background}"/>
	</g:HTMLPanel>
</ui:UiBinder> 
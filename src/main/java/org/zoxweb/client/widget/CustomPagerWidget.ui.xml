<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style type="org.zoxweb.client.widget.CustomPagerWidget.Style">
		.pageNumber_Selected {
			font-weight: bold;
			font-size: 1.1EM;
		}
		.pageNumber_NotSelected {
			font-weight: normal;
			font-size: 1EM;
		}
	</ui:style>
	<g:HTMLPanel width="100%" height="100%">
		<g:DockPanel height="2EM" width="30EM" verticalAlignment="ALIGN_MIDDLE" spacing="5">
			<g:Dock direction="WEST">
				<g:HorizontalPanel width="5EM" height="100%" verticalAlignment="ALIGN_TOP">
					<g:VerticalPanel width="2EM" height="1.5EM" ui:field="vpFirstPage" horizontalAlignment="ALIGN_RIGHT"/>
					<g:VerticalPanel horizontalAlignment="ALIGN_LEFT" width="2EM" height="1.5EM" ui:field="vpPrevious"/>
				</g:HorizontalPanel>
			</g:Dock>
			<g:Dock direction="CENTER">
				<g:HorizontalPanel height="100%" width="20EM" ui:field="hpPages" horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_MIDDLE" spacing="5"/>
			</g:Dock>
			<g:Dock direction="EAST">
				<g:HorizontalPanel verticalAlignment="ALIGN_TOP" horizontalAlignment="ALIGN_RIGHT" width="5EM" height="100%">
					<g:VerticalPanel verticalAlignment="ALIGN_MIDDLE" width="2EM" height="1.5EM" ui:field="vpNext" horizontalAlignment="ALIGN_RIGHT"/>
					<g:VerticalPanel width="2EM" height="1.5EM" horizontalAlignment="ALIGN_LEFT" ui:field="vpLastPage"/>
				</g:HorizontalPanel>
			</g:Dock>
		</g:DockPanel>
	</g:HTMLPanel>
</ui:UiBinder>
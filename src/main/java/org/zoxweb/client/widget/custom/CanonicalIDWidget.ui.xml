<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.important {
		font-weight: bold;
		color: #2E2E2E;
	}
	</ui:style>
	<g:HTMLPanel width="100%" height="100%">
		<g:HorizontalPanel width="23EM" height="2EM" verticalAlignment="ALIGN_MIDDLE">
			<g:Label text="Canonical ID:" width="7EM" height="1.5EM" styleName="{style.important}"/>
			<g:TextBox width="16.25EM" height="1.5EM" ui:field="tbCanonicalID"/>
		</g:HorizontalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 
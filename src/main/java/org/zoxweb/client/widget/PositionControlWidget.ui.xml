<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.button {
		border: 1px solid #000000;
		background-color: #F2F2F2;
     }
	</ui:style>
	<g:HTMLPanel>
		<g:HorizontalPanel width="5EM" height="2.5EM" verticalAlignment="ALIGN_MIDDLE">
			<g:PushButton width="1.5EM" height="2.5EM" ui:field="pbLeft" styleName="{style.button}"/>
		<g:VerticalPanel width="2.5EM" height="2.5EM" verticalAlignment="ALIGN_TOP" horizontalAlignment="ALIGN_CENTER">
			<g:PushButton width="1.5EM" height="1EM" ui:field="pbUp" styleName="{style.button}"/>
			<g:PushButton width="1.5EM" height="1EM" ui:field="pbDown" styleName="{style.button}"/>
		</g:VerticalPanel>
			<g:PushButton width="1.5EM" height="2.5EM" ui:field="pbRight" styleName="{style.button}"/>
		</g:HorizontalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 